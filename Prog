#include<stdio.h>
int main()
{
int a,b,c,e;
Printf("Welcome to basic mathematic operation");
printf("Enter the value of a: ");
scanf("%d\n",&a);
printf("Enter the value of b: ");
scanf("%d\n",&b);
c = a+b;
printf("Addition of values of a and b is %d",c);
e = a*b;
printf("Multiplication of values of a and b is %d",e)
return 0;
}